{
  "name": "geomath",
  "version": "1.1.0",
  "description": "AMD Geometry and Matrix modules using reuse pattern for better performance",
  "author": "SPA Tools",
  "license": "MIT",
  "main": "math.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/spatools/math.git"
  },
  "bugs": {
    "url": "https://github.com/spatools/math/issues"
  },
  "scripts": {
    "test": "grunt"
  },
  "keywords": [
    "math",
    "geometry",
    "matrix",
    "require",
    "reuse",
    "css",
    "webgl",
    "transformation",
    "umd"
  ],
  "exports": {
    ".": {
      "default": "./math.js",
      "import": "./es/math.js"
    },
    "./math/base": {
      "default": "./math/base.js",
      "import": "./es/math/base.js"
    },
    "./math/geometry": {
      "default": "./math/geometry.js",
      "import": "./es/math/geometry.js"
    },
    "./math/matrix": {
      "default": "./math/matrix.js",
      "import": "./es/math/matrix.js"
    },
    "./math/matrix3": {
      "default": "./math/matrix3.js",
      "import": "./es/math/matrix3.js"
    },
    "./math/matrix4": {
      "default": "./math/matrix4.js",
      "import": "./es/math/matrix4.js"
    },
    "./math/vector2": {
      "default": "./math/vector2.js",
      "import": "./es/math/vector2.js"
    },
    "./math/vector3": {
      "default": "./math/vector3.js",
      "import": "./es/math/vector3.js"
    }
  },
  "devDependencies": {
    "grunt": "^1.0.1",
    "grunt-build-control": "^0.7.1",
    "grunt-contrib-clean": "^1.0.0",
    "grunt-contrib-connect": "^1.0.2",
    "grunt-contrib-watch": "^1.0.0",
    "grunt-ts": "^6.0.0-beta.11",
    "grunt-tslint": "^4.0.0",
    "jit-grunt": "^0.10.0",
    "time-grunt": "^1.4.0",
    "tslint": "^4.4.2",
    "typescript": "^2.1.5"
  }
}